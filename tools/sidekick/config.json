{
  "project": "franklin-asset-selector",
  "host": "www.mymydomain.prod",
  "plugins": [
    { 
      "id": "asset-library", 
      "title": "AEM Assets Library", 
      "environments": ["edit"], 
      "url": "https://experience.adobe.com/solutions/CQ-helix-assets-addon/static-assets/resources/asset-selector.html?extConfigUrl=https://<your-config-server>/config.json", 
      "newTab": true,
      "includePaths": ["**.docx**"], 
      "passConfig": true
    }
  ],
  "blocks": {
    "video": [
      {
        "mimeType": "video/*",
        "value": "Core Embed"
      }
    ]
  },
  "filterSchema": [ 
    { 
      "header": "Assets Tags", 
      "groupKey": "AssetTagsGroup", 
      "fields": [ 
        { 
          "element": "taggroup", 
          "name": "property=metadata.application.xcm:keywords.id", 
          "columns": 3 
        } 
      ] 
    } 
  ]
}
